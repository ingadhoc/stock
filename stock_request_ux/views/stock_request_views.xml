<?xml version="1.0"?>
<odoo>
    <record id="view_stock_request_form" model="ir.ui.view">
        <field name="name">stock.request.form</field>
        <field name="model">stock.request</field>
        <field name="inherit_id" ref="stock_request.view_stock_request_form"/>
        <field name="arch" type="xml">
            <sheet>
                <label for="move_ids" groups="base.group_no_one"/>
                <field name="move_ids" readonly="1" groups="base.group_no_one"/>
                <label for="allocation_ids" groups="base.group_no_one"/>
                <field name="allocation_ids" readonly="1" groups="base.group_no_one"/>
            </sheet>
        </field>
    </record>
</odoo>
